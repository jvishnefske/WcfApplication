<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <!-- ADD appSettings section for gRPC API address -->
  <appSettings>
    <!-- IMPORTANT: Set this to the actual address of your ContactsApi gRPC endpoint -->
    <!-- For local development, this is typically https://localhost:7001 -->
    <add key="GrpcApiAddress" value="https://localhost:7001" />
  </appSettings>
</configuration>
